const std = @import("std");

let greeting: string = "Hello, {}! From {}";
let name = "Foo";
let year = 2023;

# Formats greeting with name and year
$ fn (string, string, isize) -> string
const greet = (greeting, name, year) => {
    return std/fmt.string(greeting, name, year);
};

greet(greeting, name, year) 
    |> std/fmt.println();
